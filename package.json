    "@types/busboy": "^1.3.0",
    "@types/express": "^4.17.13",
    "@types/node": "^17.0.16",
    "@typescript-eslint/eslint-plugin": "4.33.0",
    "@typescript-eslint/parser": "4.33.0",
    "eslint": "7.32.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-prettier": "4.0.0",
    "jest": "27.2.5",
    "nodemon": "^2.0.15",
    "prettier": "2.4.1",
    "typescript": "^4.5.5"
